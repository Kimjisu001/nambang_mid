<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.nambang.notice.mapper.NoticeMapper">

<select id="selectList">
select *
from notice_ck
</select>

<select id="selectNotice" parameterType="int" resultType="co.nambang.notice.vo.NoticeVO">
	select *
	from notice_ck
	where notice_no = #{noticeNo}
</select>

<select id="noticeBoard" parameterType="int" resultType="co.nambang.notice.vo.NoticeVO">
select notice_title,
       notice_writer,
       notice_creation,
       notice_content
from notice_ck
where notice_no = (select notice_no
                        from notice_ck
                        where notice_no = #{noticeNo})
</select>

<select id="selectList2" parameterType="int" resultType="co.nambang.notice.vo.NoticeVO">
	select *
	from notice_ck
	where notice_no = #{noticeNo}
</select>

</mapper>